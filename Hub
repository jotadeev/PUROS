-- StarterGui.UIPuros.MainFrame.Misc.MiscFrame.Option3.Fov.FovBox.Fov
local function C_8e()
local script = G2L["8e"];
	-- LocalScript dentro do TextBox
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	
	local textBox = script.Parent
	local minFOV = 70
	local maxFOV = 120
	
	local lockedFOV = 70 -- FOV inicial
	
	-- Funo para atualizar o FOV com base no TextBox
	local function updateFOV()
		local value = tonumber(textBox.Text)
		if value then
			lockedFOV = math.clamp(value, minFOV, maxFOV)
			camera.FieldOfView = lockedFOV
		end
	end
	
	-- Atualiza o FOV enquanto o jogador digita
	textBox:GetPropertyChangedSignal("Text"):Connect(updateFOV)
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			updateFOV()
		end
	end)
	
	-- Loop para "travar" o FOV, mesmo se outro script tentar mudar
	game:GetService("RunService").RenderStepped:Connect(function()
		if camera.FieldOfView ~= lockedFOV then
			camera.FieldOfView = lockedFOV
		end
	end)
	
end;
task.spawn(C_8e);
