-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.KeyBoxAim.Aimbot
local function C_4a()
local script = G2L["4a"];
	-- Aimbot robusto: pega inimigos do outro time, suavizado, sem raycast, LMB desativado
	
	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local textBox = script.Parent
	
	local aimbotKey = nil
	local holding = false
	local focusConnection
	
	-- DEFAULTS / LIMITES
	local DEFAULT_SMOOTH = 0.04
	local MIN_SMOOTH = 0.05
	local MAX_SMOOTH = 0.45
	
	-- Smoothness storage: NumberValue chamado "SmoothnessValue"
	local SmoothnessValue = script:FindFirstChild("SmoothnessValue")
	if not SmoothnessValue then
		-- tenta achar em PlayerGui.Aimbot (caso o GUI organize isso assim)
		local ok, gui = pcall(function()
			return player:WaitForChild("PlayerGui"):FindFirstChild("Aimbot")
		end)
		if ok and gui then
			SmoothnessValue = gui:FindFirstChild("SmoothnessValue")
		end
	end
	if not SmoothnessValue then
		SmoothnessValue = Instance.new("NumberValue")
		SmoothnessValue.Name = "SmoothnessValue"
		SmoothnessValue.Value = DEFAULT_SMOOTH
		SmoothnessValue.Parent = script
	end
	
	-- garante que o valor esteja no range
	local function clampSmooth(v)
		if v < MIN_SMOOTH then return MIN_SMOOTH end
		if v > MAX_SMOOTH then return MAX_SMOOTH end
		return v
	end
	SmoothnessValue.Value = clampSmooth(SmoothnessValue.Value)
	
	-- variavel local usada no loop (atualizada quando SmoothnessValue mudar)
	local smoothTime = SmoothnessValue.Value
	SmoothnessValue.Changed:Connect(function(newVal)
		if type(newVal) ~= "number" then return end
		smoothTime = clampSmooth(newVal)
		-- opcional: garantir que o NumberValue também esteja clampado
		if SmoothnessValue.Value ~= smoothTime then
			SmoothnessValue.Value = smoothTime
		end
	end)
	
	-- Normaliza input do TextBox (ex: "RMB" -> "mousebutton2")
	local function normalizeKeyFromText(text)
		if not text then return nil, "" end
		text = text:lower():gsub("%s+", "")
		if text == "lmb" or text == "mousebutton1" or text == "mouse1" then
			return nil, "" -- LMB desativado
		elseif text == "rmb" or text == "mousebutton2" or text == "mouse2" then
			return "mousebutton2", "RMB"
		elseif text == "mmb" or text == "mousebutton3" or text == "mouse3" then
			return "mousebutton3", "MMB"
		else
			if #text >= 1 then
				return text, text:upper()
			end
		end
		return nil, ""
	end
	
	-- Normaliza InputObject
	local function normalizeKeyFromInput(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			return nil, "" -- LMB desativado
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2", "RMB"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3", "MMB"
		elseif input.UserInputType == Enum.UserInputType.Keyboard then
			local codeName = input.KeyCode and input.KeyCode.Name
			if codeName then
				return codeName:lower(), codeName:upper()
			end
		end
		return nil, ""
	end
	
	-- Ao perder foco (config da key via textBox)
	textBox.FocusLost:Connect(function(enterPressed)
		if focusConnection then
			focusConnection:Disconnect()
			focusConnection = nil
		end
		if not enterPressed then return end
		local normalized, display = normalizeKeyFromText(textBox.Text)
		if normalized then
			aimbotKey = normalized
			textBox.Text = "Key: " .. (display or normalized:upper())
		else
			textBox.Text = "Key: ?"
		end
	end)
	
	-- Ao focar: captura qualquer tecla ou mouse (exceto LMB)
	textBox.Focused:Connect(function()
		if focusConnection then
			focusConnection:Disconnect()
		end
		focusConnection = UIS.InputBegan:Connect(function(input, processed)
			if not textBox:IsFocused() then return end
			local normalized, display = normalizeKeyFromInput(input)
			if normalized then
				aimbotKey = normalized
				textBox.Text = "Key: " .. (display or normalized:upper())
				textBox:ReleaseFocus()
				if focusConnection then
					focusConnection:Disconnect()
					focusConnection = nil
				end
			end
		end)
	end)
	
	-- Helper: converte InputObject em nome interno
	local function getInputName(input)
		if input.UserInputType == Enum.UserInputType.Keyboard then
			return input.KeyCode and input.KeyCode.Name and input.KeyCode.Name:lower() or nil
		elseif input.UserInputType == Enum.UserInputType.MouseButton1 then
			return nil -- LMB desativado
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3"
		end
		return nil
	end
	
	-- InputBegan / InputEnded para holding
	UIS.InputBegan:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if name == aimbotKey then
			-- ignora clique processado pela GUI
			if (name == "mousebutton2" or name == "mousebutton3") and processed then return end
			holding = true
		end
	end)
	
	UIS.InputEnded:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if name == aimbotKey then
			holding = false
		end
	end)
	
	-- Pega inimigo mais próximo do centro
	local function getClosestEnemyToCenter()
		local cam = camera
		local center = Vector2.new(cam.ViewportSize.X / 2, cam.ViewportSize.Y / 2)
		local closestPlayer = nil
		local closestDistance = math.huge
	
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("Head") then
				local head = plr.Character.Head
				local headPos, onScreen = cam:WorldToViewportPoint(head.Position)
				if onScreen then
					local screenPos = Vector2.new(headPos.X, headPos.Y)
					local dist = (screenPos - center).Magnitude
					if dist < closestDistance then
						closestDistance = dist
						closestPlayer = plr
					end
				end
			end
		end
		return closestPlayer
	end
	
	-- Suavização
	local function smoothAim(current, target, alpha)
		return current:Lerp(target, alpha)
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function(dt)
		if not holding then return end
		local targetPlayer = getClosestEnemyToCenter()
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
			local head = targetPlayer.Character.Head
			local cam = camera
			local dir = (head.Position - cam.CFrame.Position).Unit
			local targetCFrame = CFrame.new(cam.CFrame.Position, cam.CFrame.Position + dir)
			local alpha = math.clamp(dt / smoothTime, 0, 1)
			camera.CFrame = smoothAim(cam.CFrame, targetCFrame, alpha)
		end
	end)
	
end;
task.spawn(C_4a);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Option2.Smoothness.Check.CheckButton.Smooth
local function C_54()
local script = G2L["54"];
	-- variáveis comuns
	local bt = script.Parent
	local smb = script.Parent.Parent:WaitForChild("SmoothBox")
	
	-- sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	local hover = sounds:WaitForChild("Hover")
	local woosh = sounds:WaitForChild("Woosh")
	
	-- estado
	local BotaoClicado = false
	
	-- clique do botão
	bt.Activated:Connect(function()
		click:Play()
		BotaoClicado = not BotaoClicado
	
		smb.Visible = BotaoClicado
		bt.BackgroundTransparency = BotaoClicado and 0.35 or 1
	end)
	
end;
task.spawn(C_54);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Option2.Smoothness.Check.SmoothBox.Smoothness
local function C_58()
local script = G2L["58"];
	
	-- Serviços
	local Players = game:GetService("Players")
	
	-- Variáveis
	local player = Players.LocalPlayer
	local KeyBoxSmoo = script.Parent -- TextBox
	local AimbotScript = script.Parent.Parent.Parent.Parent.Parent.Aimbot.Frame.KeyBoxAim:WaitForChild("Aimbot") -- ajuste se necessário
	local SmoothnessValue = AimbotScript:WaitForChild("SmoothnessValue") -- NumberValue dentro do Aimbot
	
	-- Quando o jogador muda o texto e aperta Enter
	KeyBoxSmoo.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local text = KeyBoxSmoo.Text
			local num = tonumber(text)
	
			if num then
				-- Limita o valor entre 0.025 e 0.5
				if num < 0.05 then
					num = 0.05
				elseif num > 0.45 then
					num = 0.45
				end
	
				-- Atualiza o valor
				SmoothnessValue.Value = num
				KeyBoxSmoo.Text = tostring(num)
				print("Smoothness atualizado para:", num)
			else
				warn("Valor inválido digitado em Smoothness!")
				KeyBoxSmoo.Text = tostring(SmoothnessValue.Value)
			end
		end
	end)
	
end;
task.spawn(C_58);
