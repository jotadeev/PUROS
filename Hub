-- StarterGui.Model.UIPuros.MainFrame.HomeFrame.Title.LocalScript
local function C_4e()
local script = G2L["4e"];
	local textoCompleto = "Welcome," -- Texto que será digitado
	local velocidade = 0.15 -- Tempo entre cada letra (em segundos)
	
	local textLabel = script.Parent -- O TextLabel onde o texto vai aparecer
	
	-- Função para escrever letra por letra
	local function escreverTexto()
		textLabel.Text = "" -- Começa vazio
		for i = 1, #textoCompleto do
			textLabel.Text = string.sub(textoCompleto, 1, i)
			wait(velocidade)
		end
	end
	
	-- Chamando a função
	escreverTexto()
	
end;
task.spawn(C_4e);
