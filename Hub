-- StarterGui.Model.UIPuros.MainFrame.Aimbot.EspSettings.Option1.Frame.CheckButton.LocalScript
local function C_1f()
local script = G2L["1f"];
	-- Serviços
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local ts = game:GetService("TweenService")
	
	-- Sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	local hover = sounds:WaitForChild("Hover")
	local woosh = sounds:WaitForChild("Woosh")
	
	-- Variáveis UI
	local bt = script.Parent
	local cc = script.Parent.Parent.Parent:WaitForChild("ChangeColor")
	
	-- Controle
	local espAtivo = false
	local linhas = {}
	local conexoes = {
		{"Head", "UpperTorso"},
		{"UpperTorso", "LowerTorso"},
		{"UpperTorso", "LeftUpperArm"},
		{"LeftUpperArm", "LeftLowerArm"},
		{"LeftLowerArm", "LeftHand"},
		{"UpperTorso", "RightUpperArm"},
		{"RightUpperArm", "RightLowerArm"},
		{"RightLowerArm", "RightHand"},
		{"LowerTorso", "LeftUpperLeg"},
		{"LeftUpperLeg", "LeftLowerLeg"},
		{"LeftLowerLeg", "LeftFoot"},
		{"LowerTorso", "RightUpperLeg"},
		{"RightUpperLeg", "RightLowerLeg"},
		{"RightLowerLeg", "RightFoot"},
	}
	
	-- Função: criar linha
	local function criarLinha()
		local line = Drawing.new("Line")
		line.Color = _G.ESPCor or Color3.new(0, 1, 1)
		line.Thickness = 2
		line.Transparency = 1
		return line
	end
	
	-- Ativar ESP
	local function ativarESP()
		for _, target in pairs(Players:GetPlayers()) do
			if target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
				linhas[target] = {}
				for i = 1, #conexoes do
					linhas[target][i] = criarLinha()
				end
			end
		end
	end
	
	-- Desativar ESP
	local function desativarESP()
		for _, linhaTable in pairs(linhas) do
			for _, line in pairs(linhaTable) do
				line:Remove()
			end
		end
		linhas = {}
	end
	
	-- Atualizar ESP
	local function atualizarESP()
		for target, linhaTable in pairs(linhas) do
			local char = target.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				local cam = workspace.CurrentCamera
				for i, conn in ipairs(conexoes) do
					local part0 = char:FindFirstChild(conn[1])
					local part1 = char:FindFirstChild(conn[2])
					local line = linhaTable[i]
					if part0 and part1 then
						local pos0, vis0 = cam:WorldToViewportPoint(part0.Position)
						local pos1, vis1 = cam:WorldToViewportPoint(part1.Position)
						if vis0 and vis1 then
							line.From = Vector2.new(pos0.X, pos0.Y)
							line.To = Vector2.new(pos1.X, pos1.Y)
							line.Visible = true
							line.Color = _G.ESPCor or Color3.new(0, 1, 1)
						else
							line.Visible = false
						end
					else
						line.Visible = false
					end
				end
			else
				for _, line in pairs(linhaTable) do
					line.Visible = false
				end
			end
		end
	end
	
	-- Entrada principal: clique no botão
	bt.MouseButton1Click:Connect(function()
		click:Play()
		espAtivo = not espAtivo
	
		if espAtivo then
			bt.BackgroundTransparency = 0.35
			cc.Visible = true
			ativarESP()
		else
			bt.BackgroundTransparency = 1
			cc.Visible = false
			desativarESP()
		end
	end)
	
	-- Atualização contínua
	RunService.RenderStepped:Connect(function()
		if espAtivo then
			atualizarESP()
		end
	end)
	
	-- Jogadores entrando
	Players.PlayerAdded:Connect(function(plr)
		if espAtivo and plr ~= player then
			linhas[plr] = {}
			for i = 1, #conexoes do
				linhas[plr][i] = criarLinha()
			end
		end
	end)
	
	-- Jogadores saindo
	Players.PlayerRemoving:Connect(function(plr)
		if linhas[plr] then
			for _, line in pairs(linhas[plr]) do
				line:Remove()
			end
			linhas[plr] = nil
		end
	end)
	
end;
task.spawn(C_1f);
-- StarterGui.Model.UIPuros.MainFrame.Aimbot.EspSettings.Option1.ChangeColor.ColorBox.LocalScript
local function C_24()
local script = G2L["24"];
	-- Serviços
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local ts = game:GetService("TweenService")
	
	-- Sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	local hover = sounds:WaitForChild("Hover")
	local woosh = sounds:WaitForChild("Woosh")
	
	-- Variáveis UI
	local bt = script.Parent
	local cc = script.Parent.Parent.Parent:WaitForChild("ChangeColor")
	
	-- Controle
	local espAtivo = false
	local linhas = {}
	
	local conexoes = {
		{"Head", "UpperTorso"},
		{"UpperTorso", "LowerTorso"},
		{"UpperTorso", "LeftUpperArm"},
		{"LeftUpperArm", "LeftLowerArm"},
		{"LeftLowerArm", "LeftHand"},
		{"UpperTorso", "RightUpperArm"},
		{"RightUpperArm", "RightLowerArm"},
		{"RightLowerArm", "RightHand"},
		{"LowerTorso", "LeftUpperLeg"},
		{"LeftUpperLeg", "LeftLowerLeg"},
		{"LeftLowerLeg", "LeftFoot"},
		{"LowerTorso", "RightUpperLeg"},
		{"RightUpperLeg", "RightLowerLeg"},
		{"RightLowerLeg", "RightFoot"},
	}
	
	-- Função: criar linha
	local function criarLinha()
		local line = Drawing.new("Line")
		line.Color = _G.ESPCor or Color3.fromRGB(0, 255, 255) -- ciano padrão
		line.Thickness = 2
		line.Transparency = 1
		return line
	end
	
	-- Ativar ESP
	local function ativarESP()
		for _, target in pairs(Players:GetPlayers()) do
			if target ~= player and target.Character and target.Team ~= player.Team and target.Character:FindFirstChild("HumanoidRootPart") then
				linhas[target] = {}
				for i = 1, #conexoes do
					linhas[target][i] = criarLinha()
				end
			end
		end
	end
	
	-- Desativar ESP
	local function desativarESP()
		for _, linhaTable in pairs(linhas) do
			for _, line in pairs(linhaTable) do
				line:Remove()
			end
		end
		linhas = {}
	end
	
	-- Atualizar ESP
	local function atualizarESP()
		for target, linhaTable in pairs(linhas) do
			local char = target.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				local cam = workspace.CurrentCamera
				for i, conn in ipairs(conexoes) do
					local part0 = char:FindFirstChild(conn[1])
					local part1 = char:FindFirstChild(conn[2])
					local line = linhaTable[i]
					if part0 and part1 then
						local pos0, vis0 = cam:WorldToViewportPoint(part0.Position)
						local pos1, vis1 = cam:WorldToViewportPoint(part1.Position)
						if vis0 and vis1 then
							line.From = Vector2.new(pos0.X, pos0.Y)
							line.To = Vector2.new(pos1.X, pos1.Y)
							line.Visible = true
							line.Color = _G.ESPCor or Color3.fromRGB(0, 255, 255)
						else
							line.Visible = false
						end
					else
						line.Visible = false
					end
				end
			else
				for _, line in pairs(linhaTable) do
					line.Visible = false
				end
			end
		end
	end
	
	-- Clique no botão
	bt.MouseButton1Click:Connect(function()
		click:Play()
		espAtivo = not espAtivo
	
		if espAtivo then
			bt.BackgroundTransparency = 0.35
			cc.Visible = true
			ativarESP()
		else
			bt.BackgroundTransparency = 1
			cc.Visible = false
			desativarESP()
		end
	end)
	
	-- Atualização contínua
	RunService.RenderStepped:Connect(function()
		if espAtivo then
			atualizarESP()
		end
	end)
	
	-- Jogadores entrando
	Players.PlayerAdded:Connect(function(plr)
		if espAtivo and plr ~= player and plr.Team ~= player.Team then
			linhas[plr] = {}
			for i = 1, #conexoes do
				linhas[plr][i] = criarLinha()
			end
		end
	end)
	
	-- Jogadores saindo
	Players.PlayerRemoving:Connect(function(plr)
		if linhas[plr] then
			for _, line in pairs(linhas[plr]) do
				line:Remove()
			end
			linhas[plr] = nil
		end
	end)
	
end;
task.spawn(C_24);
-- StarterGui.Model.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.CheckButton.LocalScript
local function C_30()
local script = G2L["30"];
	-- variáveis comuns
	local bt = script.Parent
	local kb = script.Parent.Parent.KeyBoxAim
	local sp = script.Parent.Parent.Separador
	-- sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Sounds
	local click = sounds.Click
	local hover = sounds.Hover
	local woosh = sounds.Woosh
	
	BotaoClicado = false
	
	bt.Activated:Connect(function()
		click:Play()
		if BotaoClicado == false then
			BotaoClicado = true
			kb.Visible = true
			kb.LocalScript.Enabled = true
			kb.PlaceholderText = "Put the key"
			sp.Visible = true
			bt.BackgroundTransparency = 0.35
		else
			BotaoClicado = false
			kb.Visible = false
			kb.LocalScript.Enabled = false
			sp.Visible = false
			bt.BackgroundTransparency = 1
		end
	end)
end;
task.spawn(C_30);
-- StarterGui.Model.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.KeyBoxAim.LocalScript
local function C_34()
local script = G2L["34"];
	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local textBox = script.Parent
	local aimbotKey = nil
	local holding = false
	local focusConnection
	
	-- Suavização
	local smoothTime = 0.05 -- segundos para mirar totalmente
	
	local function normalizeKey(text)
		text = text:lower()
		if text == "mousebutton1" then
			return "mousebutton1", "LMB"
		elseif text == "mousebutton2" then
			return "mousebutton2", "RMB"
		elseif text == "mousebutton3" then
			return "mousebutton3", "MMB"
		elseif #text >= 1 then
			local firstChar = text:sub(1,1):upper()
			return firstChar:lower(), firstChar
		end
		return nil, ""
	end
	
	textBox.FocusLost:Connect(function(enterPressed)
		if focusConnection then
			focusConnection:Disconnect()
			focusConnection = nil
		end
	
		if not enterPressed then return end
		local inputText = textBox.Text
		if inputText and inputText ~= "" then
			local normalized, display = normalizeKey(inputText)
			if normalized then
				aimbotKey = normalized
				textBox.Text = "Key: " .. display
			else
				textBox.Text = "Key: ?"
			end
		end
	end)
	
	textBox.Focused:Connect(function()
		if focusConnection then
			focusConnection:Disconnect()
		end
	
		focusConnection = UIS.InputBegan:Connect(function(input, processed)
			if processed then return end
			if textBox:IsFocused() then
				if input.UserInputType == Enum.UserInputType.MouseButton1 or
					input.UserInputType == Enum.UserInputType.MouseButton2 or
					input.UserInputType == Enum.UserInputType.MouseButton3 then
	
					local internalName, display = normalizeKey(input.UserInputType.Name:lower())
					if internalName then
						aimbotKey = internalName
						textBox.Text = "Key: " .. display
						textBox:ReleaseFocus()
					end
				end
			end
		end)
	end)
	
	UIS.InputBegan:Connect(function(input, processed)
		if processed or not aimbotKey then return end
		local name = (input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode.Name:lower())
			or (input.UserInputType == Enum.UserInputType.MouseButton1 and "mousebutton1")
			or (input.UserInputType == Enum.UserInputType.MouseButton2 and "mousebutton2")
			or (input.UserInputType == Enum.UserInputType.MouseButton3 and "mousebutton3")
	
		if name == aimbotKey then
			holding = true
		end
	end)
	
	UIS.InputEnded:Connect(function(input, processed)
		if processed or not aimbotKey then return end
		local name = (input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode.Name:lower())
			or (input.UserInputType == Enum.UserInputType.MouseButton1 and "mousebutton1")
			or (input.UserInputType == Enum.UserInputType.MouseButton2 and "mousebutton2")
			or (input.UserInputType == Enum.UserInputType.MouseButton3 and "mousebutton3")
	
		if name == aimbotKey then
			holding = false
		end
	end)
	
	-- Função para encontrar o inimigo mais próximo do centro da tela
	local function getClosestEnemyToCenter()
		local cam = workspace.CurrentCamera
		local center = Vector2.new(cam.ViewportSize.X / 2, cam.ViewportSize.Y / 2)
		local closestPlayer = nil
		local closestDistance = math.huge
	
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("Head") then
				local headPos, onScreen = cam:WorldToViewportPoint(plr.Character.Head.Position)
				if onScreen then
					local screenPos = Vector2.new(headPos.X, headPos.Y)
					local dist = (screenPos - center).Magnitude
					if dist < closestDistance then
						closestDistance = dist
						closestPlayer = plr
					end
				end
			end
		end
	
		return closestPlayer
	end
	
	-- Suavização com interpolação linear (Lerp)
	local function smoothAim(current, target, alpha)
		return current:Lerp(target, alpha)
	end
	
	RunService.RenderStepped:Connect(function(dt)
		if holding then
			local cam = workspace.CurrentCamera
			local targetPlayer = getClosestEnemyToCenter()
	
			if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
				local targetPos = targetPlayer.Character.Head.Position
				local direction = (targetPos - cam.CFrame.Position).Unit
				local targetCFrame = CFrame.new(cam.CFrame.Position, cam.CFrame.Position + direction)
	
				local alpha = math.clamp(dt / smoothTime, 0, 1)
				local newCFrame = smoothAim(cam.CFrame, targetCFrame, alpha)
				cam.CFrame = newCFrame
			end
		end
	end)
	
end;
task.spawn(C_34);
-- StarterGui.Model.UIPuros.MainFrame.Buttons.MainButton.MainBScript
local function C_3a()
local script = G2L["3a"];
	-- variavel do botao
	local bt = script.Parent
	
	-- sons
	local sounds = script.Parent.Parent.Parent.Parent.Sounds
	local click = sounds.Click
	
	-- frames
	local frame = script.Parent.Parent.Parent.Aimbot
	local home = script.Parent.Parent.Parent.HomeFrame
	
	-- outros textos e frames
	local texto2 = script.Parent.Parent.MiscButton
	local texto3 = script.Parent.Parent.SettingsButton
	local frame3 = script.Parent.Parent.Parent.Settings
	
	bt.Activated:Connect(function()
		click:Play()
		home.Visible = false
		home.Interactable = false
		frame.Visible = true
		frame3.Visible = false
		bt.TextColor3 = Color3.new(1, 1, 1)
		texto2.TextColor3 = Color3.new(0.498039, 0.498039, 0.498039)
		texto3.TextColor3 = Color3.new(0.498039, 0.498039, 0.498039)
	end)
	
	--
end;
task.spawn(C_3a);
