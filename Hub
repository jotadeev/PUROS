-- StarterGui.UIPuros.OpenClose
local function C_a7()
local script = G2L["a7"];
	-- LocalScript (UIPuros > OpenClose)
	local uis = game:GetService("UserInputService")
	local main = script.Parent:WaitForChild("MainFrame")
	
	uis.InputBegan:Connect(function(input, gp)
		if gp then return end
		if input.KeyCode == Enum.KeyCode.RightAlt then
			main.Visible = not main.Visible -- alterna entre true e false
		end
	end)
end;
task.spawn(C_a7);
-- StarterGui.UIPuros.Key.Main.KeyBox.KeyScript
local function C_b2()
local script = G2L["b2"];
	-- Key System by Player
	local kbox = script.Parent
	local LoadBt = script.Parent.Parent.Load
	local UI = script.Parent.Parent.Parent.Parent.MainFrame
	local title = script.Parent.Parent.Title
	
	-- Lista de keys vinculadas a jogadores
	local playerKeys = {
		-- OWNER Key
		["JxtaCB"] = "OWNERKEY1", -- Perm
		["aspaszxt"] = "TESTERKEY1", -- Perm
		
		-- TESTER Key
		["vylurean"] = "TESTERKEY2",
		["seupai_eumorriconfia"] = "TESTERKEY3",
		["pq_chorar123"] = "TESTERKEY4"
		-- 1-DAY Key
		
	}
	
	-- Limpa o TextBox ao focar
	kbox.Focused:Connect(function()
		kbox.Text = ""
	end)
	
	-- Verifica key ao perder o foco
	kbox.FocusLost:Connect(function()
		local inputKey = kbox.Text:upper() -- ignora maisculas/minsculas
		local userNick = game.Players.LocalPlayer.Name
		local correctKey = playerKeys[userNick]
	
		if correctKey and inputKey == correctKey then
			-- Key correta do jogador
			LoadBt.Visible = true
		else
			-- Verifica se a key existe mas  de outro jogador
			local keyUsedByAnother = false
			for nick, key in pairs(playerKeys) do
				if inputKey == key then
					keyUsedByAnother = true
					break
				end
			end
	
			if keyUsedByAnother then
				title.Text = "Use only your key."
			else
				title.Text = "Inexistent Key."
			end
	
			task.wait(2)
			title.Text = "Key"
		end
	end)
	
	-- Carrega a UI quando clica no boto
	LoadBt.Activated:Connect(function()
		UI.Visible = true
		script.Parent.Parent.Parent:Destroy()
	end)
	
end;
task.spawn(C_b2);
-- StarterGui.UIPuros.SkyScriptr
local function C_bf()
local script = G2L["bf"];
	local bt = script.Parent.MainFrame.Misc.MiscFrame.Option4.Sky.CheckButton
	
	-- IDs da Skybox verde (ativa no incio)
	local greenSkyboxIds = {
		Back = "rbxassetid://11941775243",
		Down = "rbxassetid://11941774975",
		Front = "rbxassetid://11941774655",
		Left = "rbxassetid://11941774369",
		Right = "rbxassetid://11941774042",
		Up = "rbxassetid://11941773718"
	}
	
	-- Guardar os IDs originais
	local originalSkyboxIds = {}
	local usingGreenSky = true -- comea com o cu verde ativo
	
	-- Funo para aplicar uma skybox personalizada
	local function applySkybox(ids)
		for _, sky in pairs(game.Lighting:GetChildren()) do
			if sky:IsA("Sky") then
				sky.SkyboxBk = ids.Back
				sky.SkyboxDn = ids.Down
				sky.SkyboxFt = ids.Front
				sky.SkyboxLf = ids.Left
				sky.SkyboxRt = ids.Right
				sky.SkyboxUp = ids.Up
			end
		end
	end
	
	-- Salva os IDs originais e aplica o verde no incio
	for _, sky in pairs(game.Lighting:GetChildren()) do
		if sky:IsA("Sky") then
			originalSkyboxIds = {
				Back = sky.SkyboxBk,
				Down = sky.SkyboxDn,
				Front = sky.SkyboxFt,
				Left = sky.SkyboxLf,
				Right = sky.SkyboxRt,
				Up = sky.SkyboxUp
			}
			applySkybox(greenSkyboxIds)
		end
	end
	
	-- Alterna entre o cu verde e o original
	bt.MouseButton1Click:Connect(function()
		if usingGreenSky then
			applySkybox(originalSkyboxIds)
		else
			applySkybox(greenSkyboxIds)
		end
		usingGreenSky = not usingGreenSky
	end)
	
end;
task.spawn(C_bf);

return G2L["1"], require;
