-- StarterGui.UIPuros.Open.OpenScript
local function C_6()
local script = G2L["6"];
	-- serviços roblox
	local player = game.Players.LocalPlayer
	local framenot = script.Parent.Parent.NotificationFrame
	local ts = game:GetService("TweenService")
	local tinfo = TweenInfo.new(0.65, Enum.EasingStyle.Bounce, Enum.EasingDirection.Out)
	
	-- variáveis comuns
	local button = script.Parent
	local buttonanimrev = ts:Create(button, tinfo, {Position = UDim2.new(-1, 0,0.157, 0)})
	local frame = script.Parent.Parent.MainFrame
	
	-- animações
	local anim = ts:Create(frame, tinfo, {Size = UDim2.new(0.44, 0,0.384, 0)})
	
	-- notificação (tudo)
	local tinfoanim = TweenInfo.new(0.65, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
	local animnot = ts:Create(framenot, tinfoanim, {Position = UDim2.new(0.83, 0,0.797, 0)})
	local animnotrev = ts:Create(framenot, tinfoanim, {Position = UDim2.new(1.5, 0,0.797, 0)})
	
	--
	button.Activated:Connect(function()
		framenot.Content.Text = "Menu opened."
		animnot:Play()
		wait(0.035)
		frame.Visible = true
		anim:Play()
		buttonanimrev:Play()
		wait(3)
		animnotrev:Play()
	end)
end;
task.spawn(C_6);
-- StarterGui.UIPuros.Open.Drag
local function C_7()
local script = G2L["7"];
	local button = script.Parent
	local dragging = false
	local dragInput, mousePos, framePos
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton2 then
			dragging = true
			mousePos = input.Position
			framePos = button.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - mousePos
			button.Position = UDim2.new(
				0, framePos.X.Offset + delta.X,
				0, framePos.Y.Offset + delta.Y
			)
		end
	end)
end;
task.spawn(C_7);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option1.Check.CheckButton.ESP
local function C_15()
local script = G2L["15"];
	-- Serviços
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	
	-- GUI
	local bt = script.Parent
	local cc = script.Parent.Parent.Parent:WaitForChild("ChangeColor")
	local ln = script.Parent.Parent.Parent.Parent.Line
	local hdot = script.Parent.Parent.Parent.Parent.Option2
	local health = script.Parent.Parent.Parent.Parent.Option3
	local box = script.Parent.Parent.Parent.Parent.Option4
	
	-- Som
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- Configurações
	local espAtivo = false
	local linhas = {} -- [player] = {line1, line2, ...}
	_G.ESPCor = _G.ESPCor or Color3.fromRGB(255, 255, 255)
	
	local conexoes = {
		{"Head", "UpperTorso"},
		{"UpperTorso", "LowerTorso"},
		{"UpperTorso", "LeftUpperArm"},
		{"LeftUpperArm", "LeftLowerArm"},
		{"LeftLowerArm", "LeftHand"},
		{"UpperTorso", "RightUpperArm"},
		{"RightUpperArm", "RightLowerArm"},
		{"RightLowerArm", "RightHand"},
		{"LowerTorso", "LeftUpperLeg"},
		{"LeftUpperLeg", "LeftLowerLeg"},
		{"LeftLowerLeg", "LeftFoot"},
		{"LowerTorso", "RightUpperLeg"},
		{"RightUpperLeg", "RightLowerLeg"},
		{"RightLowerLeg", "RightFoot"},
	}
	
	-- Função para criar linha
	local function criarLinha()
		local line = Drawing.new("Line")
		line.Color = _G.ESPCor
		line.Thickness = 2
		line.Transparency = 1
		line.Visible = false
		return line
	end
	
	-- Cria ESP (linhas) para um jogador
	local function criarESPParaJogador(plr)
		if plr == player then return end
	
		-- Evita recriar linhas se já existem
		if not linhas[plr] then
			linhas[plr] = {}
			for i = 1, #conexoes do
				linhas[plr][i] = criarLinha()
			end
		end
	
		-- Atualiza ESP sempre que o Character for recriado
		plr.CharacterAdded:Connect(function()
			task.wait(0.5)
			if not espAtivo then return end
		end)
	end
	
	-- Atualiza posições dos desenhos
	local function atualizarESP()
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= player and linhas[plr] then
				local char = plr.Character
				if char and char:FindFirstChildOfClass("Humanoid") and char:FindFirstChildOfClass("Humanoid").Health > 0 then
					local timeIgual = (plr.Team == player.Team and plr.Team ~= nil)
					for i, conn in ipairs(conexoes) do
						local part0 = char:FindFirstChild(conn[1])
						local part1 = char:FindFirstChild(conn[2])
						local line = linhas[plr][i]
						if part0 and part1 and not timeIgual then
							local pos0, vis0 = camera:WorldToViewportPoint(part0.Position)
							local pos1, vis1 = camera:WorldToViewportPoint(part1.Position)
							if vis0 and vis1 then
								line.From = Vector2.new(pos0.X, pos0.Y)
								line.To = Vector2.new(pos1.X, pos1.Y)
								line.Color = _G.ESPCor
								line.Visible = true
							else
								line.Visible = false
							end
						else
							line.Visible = false
						end
					end
				else
					for _, line in pairs(linhas[plr]) do
						line.Visible = false
					end
				end
			end
		end
	end
	
	-- Limpa todos os desenhos
	local function desativarESP()
		for _, charLines in pairs(linhas) do
			for _, line in pairs(charLines) do
				line:Remove()
			end
		end
		table.clear(linhas)
	end
	
	-- Botão Toggle
	bt.MouseButton1Click:Connect(function()
		click:Play()
		espAtivo = not espAtivo
	
		cc.Visible = espAtivo
		ln.Visible = espAtivo
		hdot.Visible = espAtivo
		health.Visible = espAtivo
		box.Visible = espAtivo
	
		if espAtivo then
			bt.BackgroundTransparency = 0.35
			for _, plr in ipairs(Players:GetPlayers()) do
				criarESPParaJogador(plr)
			end
		else
			bt.BackgroundTransparency = 1
			desativarESP()
		end
	end)
	
	-- Atualização contínua
	RunService.RenderStepped:Connect(function()
		if espAtivo then
			atualizarESP()
		end
	end)
	
	-- Jogadores novos
	Players.PlayerAdded:Connect(function(plr)
		if espAtivo then
			criarESPParaJogador(plr)
		end
	end)
	
	-- Jogadores saindo
	Players.PlayerRemoving:Connect(function(plr)
		if linhas[plr] then
			for _, line in pairs(linhas[plr]) do
				line:Remove()
			end
			linhas[plr] = nil
		end
	end)
	
end;
task.spawn(C_15);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option1.ChangeColor.ColorBox.ChangeColor
local function C_1a()
local script = G2L["1a"];
	--// Serviços
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local ts = game:GetService("TweenService")
	
	--// Sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	--// UI
	local ChangeColor = script.Parent -- Este é o TextBox (onde o jogador digita "255,255,255")
	
	--// Valor padrão
	_G.ESPCor = _G.ESPCor or Color3.fromRGB(0, 255, 255)
	
	--// Função: Atualiza a cor do ESP
	local function atualizarCorRGB(text)
		-- Remove espaços e separa por vírgula
		local r, g, b = text:match("(%d+),%s*(%d+),%s*(%d+)")
		if r and g and b then
			r, g, b = tonumber(r), tonumber(g), tonumber(b)
			-- Garante que os valores estão entre 0 e 255
			if r >= 0 and r <= 255 and g >= 0 and g <= 255 and b >= 0 and b <= 255 then
				_G.ESPCor = Color3.fromRGB(r, g, b)
				click:Play()
				ts:Create(ChangeColor, TweenInfo.new(0.2), {TextColor3 = _G.ESPCor}):Play()
			else
				warn("Valores RGB devem estar entre 0 e 255")
			end
		else
			warn("Formato inválido! Use o formato: 255,255,255")
		end
	end
	
	--// Quando o jogador pressionar Enter no TextBox
	ChangeColor.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			atualizarCorRGB(ChangeColor.Text)
		end
	end)
	
end;
task.spawn(C_1a);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option2.HeadDot.Check.CheckButton.HdDot
local function C_22()
local script = G2L["22"];
	-- // SERVIÇOS
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	-- // VARIÁVEIS
	local player = Players.LocalPlayer
	local bt = script.Parent
	
	-- // SONS
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- // CONFIG
	local ativo = false
	local dots = {} -- [player] = círculo (Drawing object)
	
	-- // FUNÇÃO: Criar círculo (apenas contorno)
	local function criarDot()
		local circle = Drawing.new("Circle")
		circle.Color = Color3.fromRGB(255, 255, 255)
		circle.Thickness = 1.5
		circle.NumSides = 30
		circle.Radius = 8 -- tamanho fixo na tela
		circle.Filled = false
		circle.Transparency = 1
		circle.Visible = false
		return circle
	end
	
	-- // FUNÇÃO: Criar Head Dot para jogador
	local function criarHeadDot(plr)
		if plr == player then return end
		if plr.Team and player.Team and plr.Team == player.Team then return end
		if dots[plr] then return end
	
		local circle = criarDot()
		dots[plr] = circle
	
		plr.CharacterAdded:Connect(function(char)
			local humanoid = char:WaitForChild("Humanoid", 10)
			if humanoid then
				humanoid.Died:Connect(function()
					if dots[plr] then
						dots[plr].Visible = false
					end
				end)
			end
		end)
	end
	
	-- // FUNÇÃO: Atualizar posição dos círculos
	local function atualizarDots()
		for plr, circle in pairs(dots) do
			local char = plr.Character
			if char and plr.Team ~= player.Team then
				local head = char:FindFirstChild("Head")
				local humanoid = char:FindFirstChildOfClass("Humanoid")
				if head and humanoid and humanoid.Health > 0 then
					local pos, onScreen = camera:WorldToViewportPoint(head.Position)
	
					if onScreen then
						-- Posição fixa em pixels
						circle.Position = Vector2.new(pos.X, pos.Y)
						circle.Radius = 8 -- tamanho fixo na tela
						circle.Visible = true
					else
						circle.Visible = false
					end
				else
					circle.Visible = false
				end
			else
				circle.Visible = false
			end
		end
	end
	
	-- // BOTÃO TOGGLE
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
	
		if ativo then
			bt.BackgroundTransparency = 0.35
			for _, plr in pairs(Players:GetPlayers()) do
				criarHeadDot(plr)
			end
		else
			bt.BackgroundTransparency = 1
			for _, circle in pairs(dots) do
				circle:Remove()
			end
			table.clear(dots)
		end
	end)
	
	-- // Atualização contínua
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarDots()
		end
	end)
	
	-- // Jogadores novos
	Players.PlayerAdded:Connect(function(plr)
		if ativo then
			criarHeadDot(plr)
		end
	end)
	
	-- // Jogadores saindo
	Players.PlayerRemoving:Connect(function(plr)
		if dots[plr] then
			dots[plr]:Remove()
			dots[plr] = nil
		end
	end)
	
end;
task.spawn(C_22);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option3.Health.Check.CheckButton.Health
local function C_2a()
local script = G2L["2a"];
	
	-- Barra de HP vertical, fina, lado esquerdo do personagem, tamanho fixo na tela (sem distorção)
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local bt = script.Parent
	
	-- Sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- Config
	local ativo = false
	local barras = {}
	
	-- Dimensões e posição
	local larguraBarra = 2
	local offsetX = -20 -- distância da esquerda da box
	
	-- Cria barra de HP + outline
	local function criarBarra()
		local outline = Drawing.new("Square")
		outline.Color = Color3.fromRGB(0, 0, 0)
		outline.Thickness = 0.4
		outline.Filled = false
		outline.Visible = false
	
		local bar = Drawing.new("Square")
		bar.Color = Color3.fromRGB(0, 255, 0)
		bar.Filled = true
		bar.Visible = false
	
		return {Bar = bar, Outline = outline}
	end
	
	-- Remove todas as barras
	local function removerBarras()
		for _, pair in pairs(barras) do
			if pair.Bar then pair.Bar:Remove() end
			if pair.Outline then pair.Outline:Remove() end
		end
		table.clear(barras)
	end
	
	-- Atualiza posição e tamanho das barras
	local function atualizarBarras()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character then
				local char = plr.Character
				local hrp = char:FindFirstChild("HumanoidRootPart")
				local head = char:FindFirstChild("Head")
				local humanoid = char:FindFirstChildOfClass("Humanoid")
	
				if not (hrp and head and humanoid and humanoid.Health > 0) then
					if barras[plr] then
						barras[plr].Bar.Visible = false
						barras[plr].Outline.Visible = false
					end
					continue
				end
	
				-- Pegar topo e base do corpo
				local posHead = camera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.3, 0))
				local posFeet = camera:WorldToViewportPoint(hrp.Position - Vector3.new(0, 3, 0))
	
				if posFeet.Z > 0 then
					local alturaTotal = math.abs(posHead.Y - posFeet.Y)
					local hpPercent = math.clamp(humanoid.Health / humanoid.MaxHealth, 0, 1)
					local alturaHP = alturaTotal * hpPercent
	
					local x = posFeet.X + offsetX
					local yBase = posFeet.Y - alturaTotal
	
					local pair = barras[plr] or criarBarra()
					local bar = pair.Bar
					local outline = pair.Outline
	
					-- Outline
					outline.Size = Vector2.new(larguraBarra + 2, alturaTotal + 2)
					outline.Position = Vector2.new(x - 1, yBase - 1)
					outline.Visible = true
	
					-- Barra de HP
					bar.Size = Vector2.new(larguraBarra, alturaHP)
					bar.Position = Vector2.new(x, yBase + (alturaTotal - alturaHP))
					bar.Color = Color3.fromRGB(
						255 * (1 - hpPercent),
						255 * hpPercent,
						0
					)
					bar.Visible = true
	
					barras[plr] = pair
				elseif barras[plr] then
					barras[plr].Bar.Visible = false
					barras[plr].Outline.Visible = false
				end
			elseif barras[plr] then
				barras[plr].Bar.Visible = false
				barras[plr].Outline.Visible = false
			end
		end
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarBarras()
		end
	end)
	
	-- Toggle
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
		bt.BackgroundTransparency = ativo and 0.35 or 1
		if not ativo then removerBarras() end
	end)
	
	-- Limpeza ao sair do jogador
	Players.PlayerRemoving:Connect(function(plr)
		if barras[plr] then
			if barras[plr].Bar then barras[plr].Bar:Remove() end
			if barras[plr].Outline then barras[plr].Outline:Remove() end
			barras[plr] = nil
		end
	end)
	
end;
task.spawn(C_2a);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option4.Box.Check.CheckButton.Box
local function C_31()
local script = G2L["31"];
	
	-- Box ESP ajustada ao personagem, tamanho estável na tela (sem distorção)
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local bt = script.Parent
	
	-- Sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- Config
	local ativo = false
	local boxes = {}
	
	-- Função para criar box com outline
	local function criarBox()
		local outline = Drawing.new("Square")
		outline.Color = Color3.fromRGB(0, 0, 0)
		outline.Thickness = 0.6
		outline.Filled = false
		outline.Visible = false
	
		local box = Drawing.new("Square")
		box.Color = Color3.fromRGB(255, 255, 255)
		box.Thickness = 1
		box.Filled = false
		box.Visible = false
	
		return {Box = box, Outline = outline}
	end
	
	-- Remover todas as boxes
	local function removerBoxes()
		for _, pair in pairs(boxes) do
			if pair.Box then pair.Box:Remove() end
			if pair.Outline then pair.Outline:Remove() end
		end
		table.clear(boxes)
	end
	
	-- Atualizar posição das boxes
	local function atualizarBoxes()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character then
				local char = plr.Character
				local hrp = char:FindFirstChild("HumanoidRootPart")
				local head = char:FindFirstChild("Head")
				local humanoid = char:FindFirstChildOfClass("Humanoid")
	
				if not (hrp and head and humanoid and humanoid.Health > 0) then
					if boxes[plr] then
						boxes[plr].Box.Visible = false
						boxes[plr].Outline.Visible = false
					end
					continue
				end
	
				-- Pegar topo e base do corpo
				local posHead = camera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.3, 0))
				local posFeet = camera:WorldToViewportPoint(hrp.Position - Vector3.new(0, 3, 0))
	
				if posFeet.Z > 0 then
					local altura = math.abs(posHead.Y - posFeet.Y)
					local largura = altura / 2
	
					local x = posFeet.X - (largura / 2)
					local y = posHead.Y
	
					local pair = boxes[plr] or criarBox()
					local box = pair.Box
					local outline = pair.Outline
	
					box.Size = Vector2.new(largura, altura)
					box.Position = Vector2.new(x, y)
					box.Visible = true
	
					outline.Size = box.Size + Vector2.new(2, 2)
					outline.Position = box.Position - Vector2.new(1, 1)
					outline.Visible = true
	
					boxes[plr] = pair
				elseif boxes[plr] then
					boxes[plr].Box.Visible = false
					boxes[plr].Outline.Visible = false
				end
			elseif boxes[plr] then
				boxes[plr].Box.Visible = false
				boxes[plr].Outline.Visible = false
			end
		end
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarBoxes()
		end
	end)
	
	-- Toggle ESP
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
		bt.BackgroundTransparency = ativo and 0.35 or 1
		if not ativo then removerBoxes() end
	end)
	
	-- Limpeza ao sair do jogador
	Players.PlayerRemoving:Connect(function(plr)
		if boxes[plr] then
			if boxes[plr].Box then boxes[plr].Box:Remove() end
			if boxes[plr].Outline then boxes[plr].Outline:Remove() end
			boxes[plr] = nil
		end
	end)
	
end;
task.spawn(C_31);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.CheckButton.LocalScript
local function C_3c()
local script = G2L["3c"];
	-- variáveis comuns
	local bt = script.Parent
	local kb = script.Parent.Parent.KeyBoxAim
	local sp = script.Parent.Parent.Separador
	local sm = script.Parent.Parent.Parent.Parent.Option2
	-- sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Sounds
	local click = sounds.Click
	local hover = sounds.Hover
	local woosh = sounds.Woosh
	
	BotaoClicado = false
	
	bt.Activated:Connect(function()
		click:Play()
		if BotaoClicado == false then
			BotaoClicado = true
			kb.Visible = true
			sp.Visible = true
			sm.Visible = true
			bt.BackgroundTransparency = 0.35
		else
			BotaoClicado = false
			kb.Visible = false
			sp.Visible = false
			sm.Visible = false
			bt.BackgroundTransparency = 1
		end
	end)
end;
task.spawn(C_3c);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.KeyBoxAim.Aimbot
local function C_40()
local script = G2L["40"];
	-- Aimbot robusto: pega inimigos do outro time, suavizado, sem raycast, LMB desativado
	
	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local textBox = script.Parent
	
	local aimbotKey = nil
	local holding = false
	local focusConnection
	
	-- DEFAULTS / LIMITES
	local DEFAULT_SMOOTH = 0.04
	local MIN_SMOOTH = 0.05
	local MAX_SMOOTH = 0.45
	
	-- Smoothness storage: NumberValue chamado "SmoothnessValue"
	local SmoothnessValue = script:FindFirstChild("SmoothnessValue")
	if not SmoothnessValue then
		-- tenta achar em PlayerGui.Aimbot (caso o GUI organize isso assim)
		local ok, gui = pcall(function()
			return player:WaitForChild("PlayerGui"):FindFirstChild("Aimbot")
		end)
		if ok and gui then
			SmoothnessValue = gui:FindFirstChild("SmoothnessValue")
		end
	end
	if not SmoothnessValue then
		SmoothnessValue = Instance.new("NumberValue")
		SmoothnessValue.Name = "SmoothnessValue"
		SmoothnessValue.Value = DEFAULT_SMOOTH
		SmoothnessValue.Parent = script
	end
	
	-- garante que o valor esteja no range
	local function clampSmooth(v)
		if v < MIN_SMOOTH then return MIN_SMOOTH end
		if v > MAX_SMOOTH then return MAX_SMOOTH end
		return v
	end
	SmoothnessValue.Value = clampSmooth(SmoothnessValue.Value)
	
	-- variavel local usada no loop (atualizada quando SmoothnessValue mudar)
	local smoothTime = SmoothnessValue.Value
	SmoothnessValue.Changed:Connect(function(newVal)
		if type(newVal) ~= "number" then return end
		smoothTime = clampSmooth(newVal)
		-- opcional: garantir que o NumberValue também esteja clampado
		if SmoothnessValue.Value ~= smoothTime then
			SmoothnessValue.Value = smoothTime
		end
	end)
	
	-- Normaliza input do TextBox (ex: "RMB" -> "mousebutton2")
	local function normalizeKeyFromText(text)
		if not text then return nil, "" end
		text = text:lower():gsub("%s+", "")
		if text == "lmb" or text == "mousebutton1" or text == "mouse1" then
			return nil, "" -- LMB desativado
		elseif text == "rmb" or text == "mousebutton2" or text == "mouse2" then
			return "mousebutton2", "RMB"
		elseif text == "mmb" or text == "mousebutton3" or text == "mouse3" then
			return "mousebutton3", "MMB"
		else
			if #text >= 1 then
				return text, text:upper()
			end
		end
		return nil, ""
	end
	
	-- Normaliza InputObject
	local function normalizeKeyFromInput(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			return nil, "" -- LMB desativado
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2", "RMB"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3", "MMB"
		elseif input.UserInputType == Enum.UserInputType.Keyboard then
			local codeName = input.KeyCode and input.KeyCode.Name
			if codeName then
				return codeName:lower(), codeName:upper()
			end
		end
		return nil, ""
	end
	
	-- Ao perder foco (config da key via textBox)
	textBox.FocusLost:Connect(function(enterPressed)
		if focusConnection then
			focusConnection:Disconnect()
			focusConnection = nil
		end
		if not enterPressed then return end
		local normalized, display = normalizeKeyFromText(textBox.Text)
		if normalized then
			aimbotKey = normalized
			textBox.Text = "Key: " .. (display or normalized:upper())
		else
			textBox.Text = "Key: ?"
		end
	end)
	
	-- Ao focar: captura qualquer tecla ou mouse (exceto LMB)
	textBox.Focused:Connect(function()
		if focusConnection then
			focusConnection:Disconnect()
		end
		focusConnection = UIS.InputBegan:Connect(function(input, processed)
			if not textBox:IsFocused() then return end
			local normalized, display = normalizeKeyFromInput(input)
			if normalized then
				aimbotKey = normalized
				textBox.Text = "Key: " .. (display or normalized:upper())
				textBox:ReleaseFocus()
				if focusConnection then
					focusConnection:Disconnect()
					focusConnection = nil
				end
			end
		end)
	end)
	
	-- Helper: converte InputObject em nome interno
	local function getInputName(input)
		if input.UserInputType == Enum.UserInputType.Keyboard then
			return input.KeyCode and input.KeyCode.Name and input.KeyCode.Name:lower() or nil
		elseif input.UserInputType == Enum.UserInputType.MouseButton1 then
			return nil -- LMB desativado
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3"
		end
		return nil
	end
	
	-- InputBegan / InputEnded para holding
	UIS.InputBegan:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if name == aimbotKey then
			-- ignora clique processado pela GUI
			if (name == "mousebutton2" or name == "mousebutton3") and processed then return end
			holding = true
		end
	end)
	
	UIS.InputEnded:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if name == aimbotKey then
			holding = false
		end
	end)
	
	-- Pega inimigo mais próximo do centro
	local function getClosestEnemyToCenter()
		local cam = camera
		local center = Vector2.new(cam.ViewportSize.X / 2, cam.ViewportSize.Y / 2)
		local closestPlayer = nil
		local closestDistance = math.huge
	
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("Head") then
				local head = plr.Character.Head
				local headPos, onScreen = cam:WorldToViewportPoint(head.Position)
				if onScreen then
					local screenPos = Vector2.new(headPos.X, headPos.Y)
					local dist = (screenPos - center).Magnitude
					if dist < closestDistance then
						closestDistance = dist
						closestPlayer = plr
					end
				end
			end
		end
		return closestPlayer
	end
	
	-- Suavização
	local function smoothAim(current, target, alpha)
		return current:Lerp(target, alpha)
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function(dt)
		if not holding then return end
		local targetPlayer = getClosestEnemyToCenter()
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
			local head = targetPlayer.Character.Head
			local cam = camera
			local dir = (head.Position - cam.CFrame.Position).Unit
			local targetCFrame = CFrame.new(cam.CFrame.Position, cam.CFrame.Position + dir)
			local alpha = math.clamp(dt / smoothTime, 0, 1)
			camera.CFrame = smoothAim(cam.CFrame, targetCFrame, alpha)
		end
	end)
	
end;
task.spawn(C_40);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Option2.Smoothness.Check.CheckButton.Smooth
local function C_49()
local script = G2L["49"];
	-- variáveis comuns
	local bt = script.Parent
	local smb = script.Parent.Parent:WaitForChild("SmoothBox")
	
	-- sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	local hover = sounds:WaitForChild("Hover")
	local woosh = sounds:WaitForChild("Woosh")
	
	-- estado
	local BotaoClicado = false
	
	-- clique do botão
	bt.Activated:Connect(function()
		click:Play()
		BotaoClicado = not BotaoClicado
	
		smb.Visible = BotaoClicado
		bt.BackgroundTransparency = BotaoClicado and 0.35 or 1
	end)
	
