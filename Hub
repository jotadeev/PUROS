-- StarterGui.Model.UIPuros.MainFrame.Settings.SettingsFrame.Option1.Frame.CheckButton.LocalScript
local function C_46()
local script = G2L["46"];
	-- variáveis comuns
	local bt = script.Parent
	-- sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Sounds
	local click = sounds.Click
	local hover = sounds.Hover
	local woosh = sounds.Woosh
	
	BotaoClicado = false
	
	bt.Activated:Connect(function()
		click:Play()
		if BotaoClicado == false then
			BotaoClicado = true
			bt.BackgroundTransparency = 0.35
		else
			BotaoClicado = false
			bt.BackgroundTransparency = 1
		end
	end)
end;
