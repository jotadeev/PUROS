-- StarterGui.UIPuros.MainFrame.HomeFrame.ImageLabel.LocalScript
local function C_6a()
local script = G2L["6a"];
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- Referência ao ImageLabel que você quer alterar
	local imageLabel = script.Parent -- ou: game.StarterGui.ScreenGui.ImageLabel
	
	-- Pega o ID do avatar do jogador
	local userId = player.UserId
	
	-- Monta o link para a imagem do avatar
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	-- Coloca a imagem no ImageLabel
	imageLabel.Image = content
	
end;
task.spawn(C_6a);
-- StarterGui.UIPuros.MainFrame.Misc.MiscFrame.Option1.Topbar.CheckButton.Topbar
local function C_7b()
local script = G2L["7b"];
	-- variáveis comuns
	local bt = script.Parent
	local smb = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Topbar")
	
	-- sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	local hover = sounds:WaitForChild("Hover")
	local woosh = sounds:WaitForChild("Woosh")
	
	-- estado inicial 
	local BotaoClicado = true
	
	-- clique do botão
	bt.Activated:Connect(function()
		click:Play()
		BotaoClicado = not BotaoClicado 
	
		smb.Visible = BotaoClicado
		bt.BackgroundTransparency = BotaoClicado and 0.35 or 1
	end)
	
end;
task.spawn(C_7b);
-- StarterGui.UIPuros.MainFrame.Misc.MiscFrame.Option3.Fov.FovBox.Fov
local function C_83()
local script = G2L["83"];
	-- LocalScript dentro do TextBox
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	
	local textBox = script.Parent
	local minFOV = 70
	local maxFOV = 120
	
	local lockedFOV = 70 -- FOV inicial
	
	-- Função para atualizar o FOV com base no TextBox
	local function updateFOV()
		local value = tonumber(textBox.Text)
		if value then
			lockedFOV = math.clamp(value, minFOV, maxFOV)
			camera.FieldOfView = lockedFOV
		end
	end
	
	-- Atualiza o FOV enquanto o jogador digita
	textBox:GetPropertyChangedSignal("Text"):Connect(updateFOV)
	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			updateFOV()
		end
	end)
	
	-- Loop para "travar" o FOV, mesmo se outro script tentar mudar
	game:GetService("RunService").RenderStepped:Connect(function()
		if camera.FieldOfView ~= lockedFOV then
			camera.FieldOfView = lockedFOV
		end
	end)
	
end;
task.spawn(C_83);
-- StarterGui.UIPuros.Topbar.Plr.LocalScript
local function C_90()
local script = G2L["90"];
	local plr = game:GetService("Players").LocalPlayer
