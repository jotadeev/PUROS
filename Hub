	KeyBoxTrigger.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local text = KeyBoxTrigger.Text
			local num = tonumber(text)
	
			if num then
				-- Limita o valor entre 0.01 e 0.45 segundo (mais rpido!)
				if num < 0.01 then
					num = 0.01
