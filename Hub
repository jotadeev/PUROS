-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option1.Check.CheckButton.ESP
local function C_20()
local script = G2L["20"];
	-- // SERVIÇOS
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local TweenService = game:GetService("TweenService")
	
	local player = Players.LocalPlayer
	local bt = script.Parent
	local cc = script.Parent.Parent.Parent:WaitForChild("ChangeColor")
	local ln = script.Parent.Parent.Parent.Parent.Line
	local hdot = script.Parent.Parent.Parent.Parent.Option2
	local health = script.Parent.Parent.Parent.Parent.Option3
	local box = script.Parent.Parent.Parent.Parent.Option4
	
	-- // SONS
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- // VARIÁVEIS
	local espAtivo = false
	local linhas = {}
	local conexoes = {
		{"Head", "UpperTorso"},
		{"UpperTorso", "LowerTorso"},
		{"UpperTorso", "LeftUpperArm"},
		{"LeftUpperArm", "LeftLowerArm"},
		{"LeftLowerArm", "LeftHand"},
		{"UpperTorso", "RightUpperArm"},
		{"RightUpperArm", "RightLowerArm"},
		{"RightLowerArm", "RightHand"},
		{"LowerTorso", "LeftUpperLeg"},
		{"LeftUpperLeg", "LeftLowerLeg"},
		{"LeftLowerLeg", "LeftFoot"},
		{"LowerTorso", "RightUpperLeg"},
		{"RightUpperLeg", "RightLowerLeg"},
		{"RightLowerLeg", "RightFoot"},
	}
	
	-- // FUNÇÃO: Criar linha
	local function criarLinha()
		local line = Drawing.new("Line")
		line.Color = _G.ESPCor or Color3.fromRGB(0, 255, 255)
		line.Thickness = 2
		line.Transparency = 1
		line.Visible = false
		return line
	end
	
	-- // FUNÇÃO: Criar ESP para um jogador específico
	local function criarESPParaJogador(plr)
		if plr == player then return end
		if not plr.Team or not player.Team then return end
		if plr.Team == player.Team then return end
	
		linhas[plr] = {}
		for i = 1, #conexoes do
			linhas[plr][i] = criarLinha()
		end
	
		-- Se o jogador morrer e renascer, recriar as linhas
		plr.CharacterAdded:Connect(function()
			if not espAtivo then return end
			-- Remove as antigas
			if linhas[plr] then
				for _, line in pairs(linhas[plr]) do
					line:Remove()
				end
			end
			-- Cria novas linhas
			linhas[plr] = {}
			for i = 1, #conexoes do
				linhas[plr][i] = criarLinha()
			end
		end)
	end
	
	-- // FUNÇÃO: Ativar ESP (para todos os inimigos)
	local function ativarESP()
		for _, target in ipairs(Players:GetPlayers()) do
			criarESPParaJogador(target)
		end
	end
	
	-- // FUNÇÃO: Desativar ESP
	local function desativarESP()
		for _, linhaTable in pairs(linhas) do
			for _, line in pairs(linhaTable) do
				line:Remove()
			end
		end
		linhas = {}
	end
	
	-- // FUNÇÃO: Atualizar ESP
	local function atualizarESP()
		local cam = workspace.CurrentCamera
		for target, linhaTable in pairs(linhas) do
			local char = target.Character
			if char and char:FindFirstChild("HumanoidRootPart") and target.Team ~= player.Team then
				for i, conn in ipairs(conexoes) do
					local part0 = char:FindFirstChild(conn[1])
					local part1 = char:FindFirstChild(conn[2])
					local line = linhaTable[i]
					if part0 and part1 then
						local pos0, vis0 = cam:WorldToViewportPoint(part0.Position)
						local pos1, vis1 = cam:WorldToViewportPoint(part1.Position)
						if vis0 and vis1 then
							line.From = Vector2.new(pos0.X, pos0.Y)
							line.To = Vector2.new(pos1.X, pos1.Y)
							line.Visible = true
							line.Color = _G.ESPCor or Color3.fromRGB(0, 255, 255)
						else
							line.Visible = false
						end
					else
						line.Visible = false
					end
				end
			else
				for _, line in pairs(linhaTable) do
					line.Visible = false
				end
			end
		end
	end
	
	-- // BOTÃO DE ATIVAÇÃO
	bt.MouseButton1Click:Connect(function()
		click:Play()
		espAtivo = not espAtivo
	
		if espAtivo then
			bt.BackgroundTransparency = 0.35
			cc.Visible = true
			ln.Visible = true
			hdot.Visible = true
			health.Visible = true
			box.Visible = true
			ativarESP()
		else
			bt.BackgroundTransparency = 1
			cc.Visible = false
			ln.Visible = false
			hdot.Visible = false
			health.Visible = false
			box.Visible = false
			desativarESP()
		end
	end)
	
	-- // LOOP DE ATUALIZAÇÃO
	RunService.RenderStepped:Connect(function()
		if espAtivo then
			atualizarESP()
		end
	end)
	
	-- // NOVOS JOGADORES
	Players.PlayerAdded:Connect(function(plr)
		if espAtivo then
			criarESPParaJogador(plr)
		end
	end)
	
	-- // JOGADORES SAINDO
	Players.PlayerRemoving:Connect(function(plr)
		if linhas[plr] then
			for _, line in pairs(linhas[plr]) do
				line:Remove()
			end
			linhas[plr] = nil
		end
	end)
	
end;
task.spawn(C_20);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option1.ChangeColor.ColorBox.ChangeColor
local function C_25()
local script = G2L["25"];
	--// Serviços
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local ts = game:GetService("TweenService")
	
	--// Sons
	local sounds = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent:WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	--// UI
	local ChangeColor = script.Parent -- Este é o TextBox (onde o jogador digita "255,255,255")
	
	--// Valor padrão
	_G.ESPCor = _G.ESPCor or Color3.fromRGB(0, 255, 255)
	
	--// Função: Atualiza a cor do ESP
	local function atualizarCorRGB(text)
		-- Remove espaços e separa por vírgula
		local r, g, b = text:match("(%d+),%s*(%d+),%s*(%d+)")
		if r and g and b then
			r, g, b = tonumber(r), tonumber(g), tonumber(b)
			-- Garante que os valores estão entre 0 e 255
			if r >= 0 and r <= 255 and g >= 0 and g <= 255 and b >= 0 and b <= 255 then
				_G.ESPCor = Color3.fromRGB(r, g, b)
				click:Play()
				ts:Create(ChangeColor, TweenInfo.new(0.2), {TextColor3 = _G.ESPCor}):Play()
			else
				warn("Valores RGB devem estar entre 0 e 255")
			end
		else
			warn("Formato inválido! Use o formato: 255,255,255")
		end
	end
	
	--// Quando o jogador pressionar Enter no TextBox
	ChangeColor.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			atualizarCorRGB(ChangeColor.Text)
		end
	end)
	
end;
task.spawn(C_25);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option2.HeadDot.Check.CheckButton.HdDot
local function C_2e()
local script = G2L["2e"];
	-- scripted by jvttatsu --
	
	-- // SERVIÇOS
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	-- // VARIÁVEIS
	local player = Players.LocalPlayer
	local bt = script.Parent  -- botão real (TextButton, não Frame)
	
	-- // SONS
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- // CONFIG
	local ativo = false
	local circulos = {}
	
	-- // FUNÇÃO: Criar círculo branco
	local function criarCirculo()
		local circle = Drawing.new("Circle")
		circle.Color = Color3.fromRGB(255, 255, 255) -- Branco
		circle.Thickness = 1.5
		circle.Radius = 5
		circle.NumSides = 32
		circle.Filled = false
		circle.Visible = false
		return circle
	end
	
	-- // FUNÇÃO: Remover todos os círculos
	local function removerCirculos()
		for _, circle in pairs(circulos) do
			if circle then
				circle.Visible = false
				circle:Remove()
			end
		end
		table.clear(circulos)
	end
	
	-- // FUNÇÃO: Atualizar posição dos círculos
	local function atualizarCirculos()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team then
				local char = plr.Character
				if char and char:FindFirstChild("Head") and char:FindFirstChildOfClass("Humanoid") then
					local head = char.Head
					local humanoid = char:FindFirstChildOfClass("Humanoid")
					if humanoid.Health > 0 then
						local pos, visivel = camera:WorldToViewportPoint(head.Position)
						if visivel then
							local circle = circulos[plr] or criarCirculo()
							local distancia = (camera.CFrame.Position - head.Position).Magnitude
							local tamanho = math.clamp(50 / distancia, 1.5, 5)
							circle.Position = Vector2.new(pos.X, pos.Y)
							circle.Radius = tamanho
							circle.Visible = true
							circulos[plr] = circle
						elseif circulos[plr] then
							circulos[plr].Visible = false
						end
					elseif circulos[plr] then
						circulos[plr].Visible = false
					end
				elseif circulos[plr] then
					circulos[plr].Visible = false
				end
			elseif circulos[plr] then
				circulos[plr].Visible = false
			end
		end
	end
	
	-- // LOOP: Atualização contínua
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarCirculos()
		end
	end)
	
	-- // EVENTO: Clique no botão (toggle visual e funcional)
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
	
		if ativo then
			bt.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- fundo branco ativo
			bt.BackgroundTransparency = 0
		else
			bt.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- fundo preto desativado
			bt.BackgroundTransparency = 1
			removerCirculos()
		end
	end)
	
	-- // LIMPAR QUANDO JOGADOR SAIR
	Players.PlayerRemoving:Connect(function(plr)
		if circulos[plr] then
			circulos[plr]:Remove()
			circulos[plr] = nil
		end
	end)
	
end;
task.spawn(C_2e);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option3.Health.Check.CheckButton.Health
local function C_36()
local script = G2L["36"];
	-- scripted by jvttatsu --
	-- Barra de HP vertical com UIStroke, tamanho ajustável
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local bt = script.Parent
	
	-- Sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- Config
	local ativo = false
	local barras = {}
	
	-- Cria barra de HP + outline
	local function criarBarra()
		local outline = Drawing.new("Square")
		outline.Color = Color3.fromRGB(0, 0, 0)
		outline.Thickness = 2
		outline.Filled = false
		outline.Visible = false
	
		local bar = Drawing.new("Square")
		bar.Color = Color3.fromRGB(0, 255, 0)
		bar.Filled = true
		bar.Visible = false
	
		return {Bar = bar, Outline = outline}
	end
	
	local function removerBarras()
		for _, pair in pairs(barras) do
			pair.Bar.Visible = false
			pair.Bar:Remove()
			pair.Outline.Visible = false
			pair.Outline:Remove()
		end
		table.clear(barras)
	end
	
	local function atualizarBarras()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("Head") and plr.Character:FindFirstChild("Humanoid") then
				local humanoid = plr.Character.Humanoid
				if humanoid.Health <= 0 then
					if barras[plr] then
						barras[plr].Bar.Visible = false
						barras[plr].Outline.Visible = false
					end
					continue
				end
	
				local head = plr.Character.Head
				local pos, visivel = camera:WorldToViewportPoint(head.Position)
				if visivel then
					local pair = barras[plr] or criarBarra()
					local bar = pair.Bar
					local outline = pair.Outline
	
					local hpPercent = humanoid.Health / humanoid.MaxHealth
	
					-- Tamanho máximo e mínimo fixos
					local alturaMax = 35
					local largura = 4
					local alturaAtual = math.clamp(alturaMax * hpPercent, 2, alturaMax)
	
					local x = pos.X - 15
					local y = pos.Y - (alturaMax / 2)
	
					-- Outline
					outline.Size = Vector2.new(largura + 2, alturaMax + 2)
					outline.Position = Vector2.new(x - 1, y - 1)
					outline.Visible = true
	
					-- Barra verde
					bar.Size = Vector2.new(largura, alturaAtual)
					bar.Position = Vector2.new(x, y + (alturaMax - alturaAtual))
					bar.Visible = true
	
					barras[plr] = pair
				elseif barras[plr] then
					barras[plr].Bar.Visible = false
					barras[plr].Outline.Visible = false
				end
			elseif barras[plr] then
				barras[plr].Bar.Visible = false
				barras[plr].Outline.Visible = false
			end
		end
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarBarras()
		end
	end)
	
	-- Toggle
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
		if ativo then
			bt.BackgroundTransparency = 0.35
		else
			bt.BackgroundTransparency = 1
			removerBarras()
		end
	end)
	
	-- Limpeza ao sair do jogador
	Players.PlayerRemoving:Connect(function(plr)
		if barras[plr] then
			barras[plr].Bar:Remove()
			barras[plr].Outline:Remove()
			barras[plr] = nil
		end
	end)
	
end;
task.spawn(C_36);
-- StarterGui.UIPuros.MainFrame.Aimbot.EspSettings.Option4.Box.Check.CheckButton.Box
local function C_3d()
local script = G2L["3d"];
	-- scripted by jvttatsu --
	-- Box ao redor do jogador inimigo, background invisível, stroke visível
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local bt = script.Parent
	
	-- Sons
	local sounds = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild("Sounds")
	local click = sounds:WaitForChild("Click")
	
	-- Config
	local ativo = false
	local boxes = {}
	
	local function criarBox()
		local outline = Drawing.new("Square")
		outline.Color = Color3.fromRGB(0, 0, 0)
		outline.Thickness = 2
		outline.Filled = false
		outline.Visible = false
	
		local box = Drawing.new("Square")
		box.Color = Color3.fromRGB(255, 255, 255)
		box.Thickness = 1
		box.Filled = false
		box.Visible = false
	
		return {Box = box, Outline = outline}
	end
	
	local function removerBoxes()
		for _, pair in pairs(boxes) do
			pair.Box.Visible = false
			pair.Outline.Visible = false
			pair.Box:Remove()
			pair.Outline:Remove()
		end
		table.clear(boxes)
	end
	
	local function atualizarBoxes()
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Humanoid") then
				local hrp = plr.Character.HumanoidRootPart
				local humanoid = plr.Character.Humanoid
				if humanoid.Health <= 0 then
					if boxes[plr] then
						boxes[plr].Box.Visible = false
						boxes[plr].Outline.Visible = false
					end
					continue
				end
	
				local pos, visivel = camera:WorldToViewportPoint(hrp.Position)
				if visivel then
					local pair = boxes[plr] or criarBox()
					local box = pair.Box
					local outline = pair.Outline
	
					-- Tamanho fixo mínimo e máximo
					local scale = 1 / math.clamp(pos.Z / 50, 0.5, 2)  -- ajusta o zoom com distância, mas limita
					local tamanhoX = math.clamp(40 * scale, 10, 80)
					local tamanhoY = math.clamp(60 * scale, 15, 120)
	
					box.Size = Vector2.new(tamanhoX, tamanhoY)
					box.Position = Vector2.new(pos.X - tamanhoX / 2, pos.Y - tamanhoY / 2)
					box.Visible = true
	
					outline.Size = box.Size + Vector2.new(2, 2)
					outline.Position = box.Position - Vector2.new(1, 1)
					outline.Visible = true
	
					boxes[plr] = pair
				elseif boxes[plr] then
					boxes[plr].Box.Visible = false
					boxes[plr].Outline.Visible = false
				end
			elseif boxes[plr] then
				boxes[plr].Box.Visible = false
				boxes[plr].Outline.Visible = false
			end
		end
	end
	
	-- Loop principal
	RunService.RenderStepped:Connect(function()
		if ativo then
			atualizarBoxes()
		end
	end)
	
	-- Toggle
	bt.MouseButton1Click:Connect(function()
		click:Play()
		ativo = not ativo
		if ativo then
			bt.BackgroundTransparency = 0.35
		else
			bt.BackgroundTransparency = 1
			removerBoxes()
		end
	end)
	
	-- Limpeza ao sair do jogador
	Players.PlayerRemoving:Connect(function(plr)
		if boxes[plr] then
			boxes[plr].Box:Remove()
			boxes[plr].Outline:Remove()
			boxes[plr] = nil
		end
	end)
	
end;
task.spawn(C_3d);
-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.CheckButton.LocalScript
local function C_46()
