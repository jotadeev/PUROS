-- StarterGui.UIPuros.Open.OpenScript
local function C_d()
local script = G2L["d"];
	-- serviços roblox
	local player = game.Players.LocalPlayer
	local framenot = script.Parent.Parent.NotificationFrame
	local ts = game:GetService("TweenService")
	local tinfo = TweenInfo.new(0.65, Enum.EasingStyle.Bounce, Enum.EasingDirection.Out)
	
	-- variáveis comuns
	local button = script.Parent
	local buttonanimrev = ts:Create(button, tinfo, {Position = UDim2.new(-1, 0,0.157, 0)})
	local frame = script.Parent.Parent.MainFrame
	
	-- animações
	local anim = ts:Create(frame, tinfo, {Size = UDim2.new(0.44, 0,0.384, 0)})
	
	-- notificação (tudo)
	local tinfoanim = TweenInfo.new(0.65, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
	local animnot = ts:Create(framenot, tinfoanim, {Position = UDim2.new(0.83, 0,0.797, 0)})
	local animnotrev = ts:Create(framenot, tinfoanim, {Position = UDim2.new(1.5, 0,0.797, 0)})
	
	--
	button.Activated:Connect(function()
		framenot.Content.Text = "Menu opened."
		animnot:Play()
		wait(0.035)
		frame.Visible = true
		anim:Play()
		buttonanimrev:Play()
		wait(3)
		animnotrev:Play()
	end)
end;
task.spawn(C_d);
-- StarterGui.UIPuros.Open.Drag
local function C_e()
local script = G2L["e"];
	local button = script.Parent
	local dragging = false
	local dragInput, mousePos, framePos
	
	button.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton2 then
			dragging = true
			mousePos = input.Position
			framePos = button.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	button.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - mousePos
			button.Position = UDim2.new(
				0, framePos.X.Offset + delta.X,
				0, framePos.Y.Offset + delta.Y
			)
		end
	end)
end;
task.spawn(C_e);
-- StarterGui.UIPuros.MainFrame.Minimize.MinScript
local function C_11()
local script = G2L["11"];
	-- serviços do roblox
	local ts = game:GetService("TweenService")
	
	-- sons
	local sounds = script.Parent.Parent.Parent.Sounds
	local click = sounds.Click
	local hover = sounds.Hover
	local woosh = sounds.Woosh
	
	-- variáveis comuns (botões, frames, etc)
	local player = game.Players.LocalPlayer
	local framenot = script.Parent.Parent.Parent.NotificationFrame
	local btmin = script.Parent
	local btopen = script.Parent.Parent.Parent.Open
	local frame = btmin.Parent
	
	-- variáveis de animação
	local tinfo = TweenInfo.new(0.25, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	local anim = ts:Create(frame, tinfo, {Size = UDim2.new(0.44, 0,0.006, 0)})
	local animopen = ts:Create(btopen, tinfo, {Position = UDim2.new(0.083, 0,0.157, 0)})
	
	-- notificação (tudo)
	local tinfoanim = TweenInfo.new(0.25, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
	local animnot = ts:Create(framenot, tinfoanim, {Position = UDim2.new(0.83, 0,0.797, 0)})
	local animnotrev = ts:Create(framenot, tinfoanim, {Position = UDim2.new(1.5, 0,0.797, 0)})
	
	-- ativar botão (Minimizar menu)
		btmin.Activated:Connect(function()
		click:Play()
		wait(0.15)
		anim:Play()
		wait(0.03)
		woosh:Play()
		wait(0.25)
		frame.Visible = false
		task.wait(0.05)
		btopen.Visible = true
		framenot.Content.Text = "Minimized the menu, you can open it clicking on the P button. "
		animnot:Play()
		animopen:Play()
		wait(4)
		animnotrev:Play()
	end)
	
	-- BOTÃO DE MINIMIZAR, SCRIPT. scripted by jvttatsu
end;
task.spawn(C_11);
