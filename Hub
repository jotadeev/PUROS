	txt.Text = "@" .. plr.DisplayName
	
end;
task.spawn(C_90);
-- StarterGui.UIPuros.OpenClose
local function C_96()
local script = G2L["96"];
	-- LocalScript (UIPuros > OpenClose)
	local uis = game:GetService("UserInputService")
	local main = script.Parent:WaitForChild("MainFrame")
	
	uis.InputBegan:Connect(function(input, gp)
		if gp then return end
		if input.KeyCode == Enum.KeyCode.RightAlt then
			main.Visible = not main.Visible -- alterna entre true e false
		end
	end)
end;
task.spawn(C_96);
-- StarterGui.UIPuros.Sky
local function C_97()
local script = G2L["97"];
	
	
	-- IDs da nova Skybox
	local skyboxIds = {
		Back = "rbxassetid://11941775243",
		Down = "rbxassetid://11941774975",
		Front = "rbxassetid://11941774655",
		Left = "rbxassetid://11941774369",
		Right = "rbxassetid://11941774042",
		Up = "rbxassetid://11941773718"
	}
	
	-- Função para trocar todos os IDs da Skybox
	local function changeSkybox()
		-- Procura todos os objetos Sky (Skybox) no jogo
		for _, sky in pairs(game.Lighting:GetChildren()) do
			if sky:IsA("Sky") then
				sky.SkyboxBk = skyboxIds.Back
				sky.SkyboxDn = skyboxIds.Down
				sky.SkyboxFt = skyboxIds.Front
				sky.SkyboxLf = skyboxIds.Left
				sky.SkyboxRt = skyboxIds.Right
				sky.SkyboxUp = skyboxIds.Up
			end
		end
	end
	
	changeSkybox()
	
end;
task.spawn(C_97);

return G2L["1"], require;
