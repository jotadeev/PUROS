-- StarterGui.UIPuros.MainFrame.Aimbot.Aimbotframe.Aimbot.Frame.KeyBoxAim.Aimbot
local function C_4a()
local script = G2L["4a"];
	-- scripted by jvttatsu --
	-- Aimbot robusto: captura teclas (inclui LMB/RMB), só mira se inimigo visível
	
	local UIS = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local workspace = game:GetService("Workspace")
	local camera = workspace.CurrentCamera
	
	local player = Players.LocalPlayer
	local textBox = script.Parent -- TextBox onde o usuário seta a key
	
	local aimbotKey = nil    -- string interna, ex: "mousebutton1", "q", "space"
	local holding = false
	local focusConnection = nil
	
	-- Suavização (em segundos)
	local smoothTime = 0.05
	
	-- Normaliza texto digitado (ex: "LMB", "mousebutton1", "q", "space")
	local function normalizeKeyFromText(text)
		if not text then return nil, "" end
		local t = text:lower():gsub("%s+", "")
		if t == "lmb" or t == "mousebutton1" or t == "mouse1" then
			return "mousebutton1", "LMB"
		elseif t == "rmb" or t == "mousebutton2" or t == "mouse2" then
			return "mousebutton2", "RMB"
		elseif t == "mmb" or t == "mousebutton3" or t == "mouse3" then
			return "mousebutton3", "MMB"
		else
			-- aceita "space", "leftshift", "q", "e", etc
			if #t >= 1 then
				return t, t:upper()
			end
		end
		return nil, ""
	end
	
	-- Normaliza um InputObject (quando o TextBox está focado e user pressiona algo)
	local function normalizeKeyFromInput(input)
		-- mouse buttons
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			return "mousebutton1", "LMB"
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2", "RMB"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3", "MMB"
			-- keyboard
		elseif input.UserInputType == Enum.UserInputType.Keyboard then
			local codeName = input.KeyCode and input.KeyCode.Name
			if codeName then
				return codeName:lower(), codeName:upper()
			end
		end
		return nil, ""
	end
	
	-- Ao perder foco: usuário pode ter digitado "q" e apertado Enter
	textBox.FocusLost:Connect(function(enterPressed)
		-- desconecta captura temporária
		if focusConnection then
			focusConnection:Disconnect()
			focusConnection = nil
		end
	
		if not enterPressed then return end
		local txt = textBox.Text
		local normalized, display = normalizeKeyFromText(txt)
		if normalized then
			aimbotKey = normalized
			textBox.Text = "Key: " .. (display or normalized:upper())
		else
			textBox.Text = "Key: ?"
		end
	end)
	
	-- Ao focar: captura InputBegan globalmente para bind — permite clicar na tela (fora/embaixo de UI) para bind
	textBox.Focused:Connect(function()
		-- remove conexão antiga se existir
		if focusConnection then
			focusConnection:Disconnect()
		end
	
		focusConnection = UIS.InputBegan:Connect(function(input, processed)
			-- só enquanto o TextBox estiver focado
			if not textBox:IsFocused() then return end
	
			-- capturar mouse buttons e teclas
			local normalized, display = normalizeKeyFromInput(input)
			if normalized then
				-- Para bind, permitimos capturar independentemente de processed (usuário quer clicar qualquer lugar para bind)
				aimbotKey = normalized
				textBox.Text = "Key: " .. (display or normalized:upper())
				textBox:ReleaseFocus()
				-- desconecta que estava capturando
				if focusConnection then
					focusConnection:Disconnect()
					focusConnection = nil
				end
				return
			end
	
			-- (caso Input não reconhecido, ignora)
		end)
	end)
	
	-- Helper: converte um InputObject recebido em nome interno compatível com aimbotKey
	local function getInputName(input)
		if input.UserInputType == Enum.UserInputType.Keyboard then
			return input.KeyCode and input.KeyCode.Name and input.KeyCode.Name:lower() or nil
		elseif input.UserInputType == Enum.UserInputType.MouseButton1 then
			return "mousebutton1"
		elseif input.UserInputType == Enum.UserInputType.MouseButton2 then
			return "mousebutton2"
		elseif input.UserInputType == Enum.UserInputType.MouseButton3 then
			return "mousebutton3"
		end
		return nil
	end
	
	-- InputBegan para ativar holding (uso normal do aimbot)
	UIS.InputBegan:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if not name then return end
	
		-- Se o input for mouse button e o clique foi processado pela UI (processed==true),
		-- então ignora (assim não ativa aimbot quando clicam em GUI).
		if (name == "mousebutton1" or name == "mousebutton2" or name == "mousebutton3") and processed then
			return
		end
	
		if name == aimbotKey then
			holding = true
		end
	end)
	
	UIS.InputEnded:Connect(function(input, processed)
		if not aimbotKey then return end
		local name = getInputName(input)
		if not name then return end
	
		if name == aimbotKey then
			holding = false
		end
	end)
	
	-- Função de visibilidade por raycast: retorna true se a câmera tem linha direta até targetPart (ex: Head)
	local function isVisible(targetPart)
		if not targetPart or not targetPart.Parent then return false end
		local origin = camera.CFrame.Position
		local dirVec = (targetPart.Position - origin)
		if dirVec.Magnitude <= 0 then return false end
	
		local raycastParams = RaycastParams.new()
		raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
		-- blacklist do próprio jogador para não colidir com nosso corpo / ferramentas
		if player.Character then
			raycastParams.FilterDescendantsInstances = {player.Character}
		else
			raycastParams.FilterDescendantsInstances = {}
		end
	
		-- realiza o raycast até a distância do target
		local result = workspace:Raycast(origin, dirVec, raycastParams)
		if not result then
			-- nada bloqueou: visível
			return true
		end
	
		-- se o primeiro objeto atingido for parte do target (ou descendente), então visível
		local hitInst = result.Instance
		if hitInst and hitInst:IsDescendantOf(targetPart.Parent) then
			return true
		end
	
		return false
	end
	
	-- Pega o inimigo mais próximo do centro da tela E visível
	local function getClosestEnemyToCenter()
		local cam = camera
		local center = Vector2.new(cam.ViewportSize.X / 2, cam.ViewportSize.Y / 2)
		local closestPlayer = nil
		local closestDistance = math.huge
	
		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player and plr.Team ~= player.Team and plr.Character and plr.Character:FindFirstChild("Head") then
				local head = plr.Character.Head
				local headPos, onScreen = cam:WorldToViewportPoint(head.Position)
				if onScreen and isVisible(head) then
					local screenPos = Vector2.new(headPos.X, headPos.Y)
					local dist = (screenPos - center).Magnitude
					if dist < closestDistance then
						closestDistance = dist
						closestPlayer = plr
					end
				end
			end
		end
	
		return closestPlayer
	end
	
	-- Suavização (lerp entre CFrames)
	local function smoothAim(current, target, alpha)
		return current:Lerp(target, alpha)
	end
	
	-- Loop principal: aplica mira suavizada enquanto holding == true
	RunService.RenderStepped:Connect(function(dt)
		if not holding then return end
		local targetPlayer = getClosestEnemyToCenter()
		if not targetPlayer then return end
		if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("Head") then return end
	
		local head = targetPlayer.Character.Head
		-- double-check vis (prevenção extra)
		if not isVisible(head) then return end
	
		local cam = camera
		local targetPos = head.Position
		local dir = (targetPos - cam.CFrame.Position)
		if dir.Magnitude <= 0 then return end
		local direction = dir.Unit
		local targetCFrame = CFrame.new(cam.CFrame.Position, cam.CFrame.Position + direction)
	
		local alpha = math.clamp(dt / smoothTime, 0, 1)
		local newCFrame = smoothAim(cam.CFrame, targetCFrame, alpha)
		camera.CFrame = newCFrame
	end)
	
end;
task.spawn(C_4a);
-- StarterGui.UIPuros.MainFrame.Buttons.MainButton.MainBScript
local function C_50()
local script = G2L["50"];
	-- variavel do botao
